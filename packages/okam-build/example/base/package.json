{
  "name": "example",
  "version": "1.0.0",
  "description": "example",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "init": "npm i && sh ./scripts/init.sh",
    "clean": "rm -rf dist/",
    "clean:wx": "rm -rf wx_dist/",
    "clean:ant": "rm -rf ant_dist/",
    "clean:tt": "rm -rf tt_dist/",
    "clean:quick": "rm -rf quick_dist/",
    "dev": "cross-env OKAM_ENV=dev NODE_ENV=dev NODE_PATH=./node_modules node scripts/build.js --watch",
    "dev:clean": "cross-env OKAM_ENV=dev NODE_ENV=dev NODE_PATH=./node_modules node scripts/build.js --watch --clean",
    "dev:server": "cross-env OKAM_ENV=dev NODE_ENV=dev NODE_PATH=./node_modules node scripts/build.js --watch --server",
    "dev:debug": "cross-env OKAM_ENV=dev NODE_ENV=dev NODE_PATH=./node_modules node --inspect-brk scripts/build.js --watch",
    "prod": "cross-env NODE_PATH=./node_modules node scripts/build.js --clean",
    "dev:wx": "npm run dev -- --type wx",
    "dev:wx:clean": "npm run dev:clean -- --type wx",
    "dev:wx:server": "npm run dev:server -- --type wx",
    "dev:wx:debug": "npm run dev:debug -- --type wx",
    "prod:wx": "npm run prod -- --type wx",
    "dev:ant": "npm run dev -- --type ant",
    "dev:ant:clean": "npm run dev:clean -- --type ant",
    "dev:ant:server": "npm run dev:server -- --type ant",
    "dev:ant:debug": "npm run dev:debug -- --type ant",
    "prod:ant": "npm run prod -- --type ant",
    "dev:tt": "npm run dev -- --type tt",
    "dev:tt:clean": "npm run dev:clean -- --type tt",
    "dev:tt:server": "npm run dev:server -- --type tt",
    "dev:tt:debug": "npm run dev:debug -- --type tt",
    "prod:tt": "npm run prod -- --type tt",
    "dev:quick": "npm run dev -- --type quick",
    "dev:quick:clean": "npm run dev:clean -- --type quick",
    "dev:quick:server": "npm run dev:server -- --type quick",
    "dev:quick:debug": "npm run dev:debug -- --type quick",
    "prod:quick": "npm run prod -- --type quick",
    "dev:h5": "npm run dev:server -- --type h5",
    "dev:h5:clean": "npm run dev:clean -- --server --type h5",
    "dev:h5:server": "npm run dev:server -- --type h5 --port 9090",
    "dev:h5:debug": "npm run dev:debug -- --server --type h5",
    "prod:h5": "npm run prod -- --type h5",
    "start:h5": "cross-env NODE_ENV=dev NODE_PATH=./node_modules node scripts/h5/build.js"
  },
  "keywords": [
    "example"
  ],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "babel-plugin-minify-dead-code-elimination": "^0.5.1",
    "lodash": "^4.17.10",
    "mini-html-parser2": "^0.1.0",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "redux": "^4.0.0",
    "redux-thunk": "^2.3.0",
    "vue": "^2.6.11",
    "vue-router": "^3.0.2"
  },
  "devDependencies": {
    "autoresponse": "^0.3.0",
    "express": "^4.16.4",
    "connect": "^3.6.6",
    "cross-env": "^6.0.0",
    "okam-plugin-tinyimg": "^0.1.4",
    "postcss-url": "^8.0.0",
    "postcss-import-sync2": "^1.1.0",
    "promise-polyfill": "^8.1.0"
  },
  "browserslist": [
    "> 1%",
    "last 2 versions",
    "not ie <= 8"
  ]
}
